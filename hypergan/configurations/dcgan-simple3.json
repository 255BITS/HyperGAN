{
  "description": "The original famous DCGAN.  Limited to 64x64x3",
  "publication": "https://arxiv.org/abs/1511.06434",
  "discriminator": 
  {
    "class": "class:hypergan.discriminators.configurable_discriminator.ConfigurableDiscriminator",
    "defaults":{
    },
    "layers":[
      "conv 64 ", 
      "prelu",
      "conv 128",
      "prelu",
      "conv 256",
      "prelu",
      "conv 512",
      "prelu",
      "flatten",
      "linear 1"
    ]
  },
 
  "latent":
  {
    "class": "function:hypergan.distributions.uniform_distribution.UniformDistribution",
    "projections": [
      "function:hypergan.distributions.uniform_distribution.identity"
    ],
    "min": -1,
    "max": 1,
    "z": 100
  },
  "generator": 
  {
    "class": "class:hypergan.generators.configurable_generator.ConfigurableGenerator",
    "defaults":{
    },
    "layers":[
      "linear 4*4*512",
      "instance_norm",
      "prelu",
      "resize_conv 256",
      "instance_norm",
      "prelu",
      "resize_conv 128",
      "instance_norm",
      "prelu",
      "resize_conv 64",
      "instance_norm",
      "prelu",
      "deconv 3",
      "instance_norm",
      "tanh"
    ]
  },
  

  "loss":
  {
    "class": "function:hypergan.losses.standard_loss.StandardLoss",
    "reduce": "reduce_mean"
  },
  "trainer": {
    "class": "function:hypergan.trainers.simultaneous_trainer.SimultaneousTrainer",
    "optimizer": {
      "class": "function:tensorflow.python.training.adam.AdamOptimizer",
      "learn_rate": 1e-4,
      "beta1":0.0,
      "beta2":0.999
    },
      "hooks":[
      {
        "class": "function:hypergan.train_hooks.stabilizing_training_train_hook.StabilizingTrainingTrainHook",
        "gamma": 1.0
      }]

  },
  "runtime": {
    "channels": 3,
    "width": 64,
    "height": 64,
    "train": "hypergan train [dataset] --sampler static_batch -b 8"
  },

  "hypergan_version": "0.10.0",
  "class": "class:hypergan.gans.standard_gan.StandardGAN"

}
